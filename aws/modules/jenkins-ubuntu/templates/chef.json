{
  "aws": {
    "access_key_id": "${aws_access_key_id}",
    "secret_access_key": "${aws_secret_access_key}",
    "region": "${aws_region}"
  },
  "jenkins": {
    "github": {
      "user": "${jenkins_github_ci_user}",
      "token": "${jenkins_github_ci_token}"
    },
    "seedjob": {
      "branch": {
        "include": "${jenkins_seedjob_branch_include}",
        "exclude": "${jenkins_seedjob_branch_exclude}",
        "trigger": "${jenkins_seedjob_branch_trigger}"
      },
      "repo": {
        "include": "${jenkins_seedjob_repo_include}",
        "exclude": "${jenkins_seedjob_repo_exclude}",
        "owner": "${jenkins_seedjob_repo_owner}"
      }
    },
    "users": {
      "developerPassword": "${jenkins_developer_password}",
      "adminPassword": "${jenkins_admin_password}",
      "email": "${jenkins_email}"
    }
  },
  "run_list": [
    "recipe[demo::default]"
  ]
}